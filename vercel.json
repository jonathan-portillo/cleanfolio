{
  "version": 2,

  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node@16"
    }
  ],

  "routes": [
    {
      "src": "/index.js",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "s-maxage=1, stale-while-revalidate"
      },
      "continue": true
    }
  ]
}
